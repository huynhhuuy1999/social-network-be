{
  "entryFile": "src/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "spec": {
    "outputDirectory": "public",
    "specVersion": 3
  },
  "controllerPathGlobs": ["src/**/controllers/*"],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/models": "./src/models/",
      "@/enum": "./src/enum/"
    },
    "types": "./src/models"
  }
}
