generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model User {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  email        String  @unique
  password     String
  firstName    String
  surname      String
  birthDate    String
  gender       Int
  refreshToken String?
  avatar       String?
}

model UserDefault {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Test {
  id   String   @id @default(auto()) @map("_id") @db.ObjectId
  name String[]
}
